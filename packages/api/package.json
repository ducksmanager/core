{
  "name": "~api",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "version": "1.0.0",
  "scripts": {
    "build": "rm -rf dist && ./node_modules/.bin/tsc && tsc-alias",
    "dev": "concurrently -n api,docker-compose,db \"pnpm -F ~api nodemon-watch\" \"docker compose -f docker-compose-dev.yml up\" \"pnpm -F ~prisma-clients dev\"",
    "nodemon-watch": "concurrently -n nodemon,tsc \"nodemon\" \"tsc --noEmit --watch\"",
    "lint": "eslint --fix --cache ."
  },
  "dependencies": {
    "@pusher/push-notifications-server": "^1.2.6",
    "@sentry/node": "^7.81.1",
    "axios": "^1.6.2",
    "cloudinary": "^1.41.0",
    "csv-parse": "^5.5.2",
    "dayjs": "^1.11.10",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.9",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.2",
    "module-alias": "^2.2.3",
    "nodemailer": "^6.9.7",
    "socket.io": "^4.7.2",
    "~dm-types": "workspace:*",
    "~prisma-clients": "workspace:*"
  },
  "devDependencies": {
    "@types/ejs": "^3.1.5",
    "@types/i18n": "^0.13.10",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^16.18.65",
    "@types/nodemailer": "^6.4.14",
    "@typescript-eslint/eslint-plugin": "^6.12.0",
    "@typescript-eslint/parser": "^6.12.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "nodemon": "^3.0.1",
    "ts-node": "^10.9.1",
    "tsc-alias": "^1.8.8",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.3.2"
  }
}