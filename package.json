{
	"private": true,
	"packageManager": "pnpm@7.4.0",
	"scripts": {
		"build": "vite build",
		"dev": "concurrently -n front,back \"pnpm run start:front\" \"pnpm run start:back\"",
		"start:front": "vite --port 8001",
		"start:back": "cd api && pnpm run dev",
		"lint": "eslint --fix",
		"typecheck": "vue-tsc --noEmit",
		"preview": "vite preview",
		"publish": "cp package.json package-real.json && mv package-publish.json package.json && pnpm publish --no-git-checks; mv package.json package-publish.json && mv package-real.json package.json",
		"test": "vitest",
		"up": "taze major -I"
	},
	"dependencies": {
		"@bperel/vue3-popper-teleport": "^1.4.3",
		"@mapbox/mapbox-gl-geocoder": "^5.0.1",
		"@popperjs/core": "^2.11.6",
		"@sentry/tracing": "^7.15.0",
		"@sentry/vue": "^7.15.0",
		"@vueuse/core": "^9.3.0",
		"@vueuse/head": "^0.9.8",
		"axios": "^0.27.2",
		"axios-cache-interceptor": "^0.10.7",
		"bootstrap": "^5.2.2",
		"bootstrap-icons-vue": "^1.8.1",
		"bootstrap-vue-3": "^0.3.11",
		"chart.js": "^3.9.1",
		"concurrently": "^7.4.0",
		"core-js": "^3.25.5",
		"js-cookie": "^3.0.1",
		"js-yaml": "^4.1.0",
		"mapbox-gl": "^2.10.0",
		"page-flip": "^2.0.7",
		"pinia": "^2.0.23",
		"timeago.js": "^4.0.2",
		"v-contextmenu": "^3.0.0",
		"vue": "^3.2.40",
		"vue-chart-3": "^3.1.8",
		"vue-i18n": "^9.2.2",
		"vue-i18n-extract": "^2.0.7",
		"vue-mapbox-ts": "^0.8.1",
		"vue-router": "^4.1.5",
		"vue-slicksort": "^2.0.0-alpha.5"
	},
	"devDependencies": {
		"@antfu/eslint-config": "^0.27.0",
		"@intlify/vite-plugin-vue-i18n": "^6.0.3",
		"@types/js-cookie": "^3.0.2",
		"@types/node": "^18.8.4",
		"@typescript-eslint/eslint-plugin": "^5.40.0",
		"@typescript-eslint/parser": "^5.40.0",
		"@vitejs/plugin-vue": "^3.1.2",
		"@vue/test-utils": "^2.1.0",
		"eslint": "^8.25.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-plugin-prettier-vue": "^4.2.0",
		"eslint-plugin-simple-import-sort": "^8.0.0",
		"eslint-plugin-vue": "^9.6.0",
		"jsdom": "^20.0.1",
		"pnpm": "^7.13.4",
		"prettier": "^2.7.1",
		"prettier-plugin-prisma": "^4.4.0",
		"sass": "^1.55.0",
		"taze": "^0.8.2",
		"typescript": "^4.8.4",
		"unplugin-auto-import": "^0.11.2",
		"unplugin-vue-components": "^0.22.8",
		"vite": "^3.1.7",
		"vite-plugin-pages": "^0.26.0",
