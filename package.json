{
  "name": "ducksmanager",
  "packageManager": "pnpm@7.4.0",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "src/components",
    "api/dist/prisma/*/*.{js,json,prisma,ts}",
    "api/prisma",
    "types"
  ],
  "scripts": {
    "build": "vite build",
    "dev": "concurrently -n front,back \"pnpm run start:front\" \"pnpm run start:back\"",
    "lint": "eslint --fix --cache .",
    "prepare": "husky install",
    "preview": "vite preview",
    "publish": "(cd api && pnpm run generate-route-types) && cp package.json package-real.json && mv package-publish.json package.json && pnpm publish --no-git-checks; mv package.json package-publish.json && mv package-real.json package.json",
    "start:back": "cd api && pnpm run dev",
    "start:front": "concurrently -n vite,vue-tsc \"vite --port 8001\" \"vue-tsc --noEmit --watch\"",
    "story:dev": "histoire dev",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I"
  },
  "dependencies": {
    "@bperel/vue3-popper-teleport": "^1.4.3",
    "@mapbox/mapbox-gl-geocoder": "^5.0.1",
    "@popperjs/core": "^2.11.8",
    "@sentry/tracing": "^7.57.0",
    "@sentry/vue": "^7.57.0",
    "@vueuse/core": "^10.2.1",
    "@vueuse/head": "^1.1.26",
    "axios": "^1.4.0",
    "axios-cache-interceptor": "^1.2.0",
    "bootstrap": "^5.3.0",
    "bootstrap-icons-vue": "^1.10.3",
    "bootstrap-vue-next": "^0.9.17",
    "chart.js": "^4.3.0",
    "core-js": "^3.31.0",
    "dayjs": "^1.11.9",
    "js-cookie": "^3.0.5",
    "js-yaml": "^4.1.0",
    "mapbox-gl": "^2.15.0",
    "page-flip": "^2.0.7",
    "pinia": "^2.1.4",
    "timeago.js": "^4.0.2",
    "v-contextmenu": "^3.0.0",
    "vue": "^3.3.4",
    "vue-chartjs": "^5.2.0",
    "vue-i18n": "^9.2.2",
    "vue-i18n-extract": "^2.0.7",
    "vue-mapbox-ts": "^0.9.8",
    "vue-router": "^4.2.2",
    "vue-slicksort": "^2.0.5"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.39.6",
    "@histoire/plugin-vue": "^0.16.1",
    "@iconify-json/bi": "^1.1.17",
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@rollup/plugin-typescript": "^11.1.2",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^18.16.19",
    "@types/page-flip": "^2.0.0",
    "@typescript-eslint/eslint-plugin": "^5.61.0",
    "@typescript-eslint/parser": "^5.61.0",
    "@vitejs/plugin-vue": "^4.2.3",
    "@vue-macros/reactivity-transform": "^0.3.10",
    "@vue/compiler-sfc": "^3.3.4",
    "@vue/test-utils": "^2.4.0",
    "concurrently": "^8.2.0",
    "ejs-lint": "^2.0.0",
    "eslint": "^8.44.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.15.1",
    "histoire": "^0.16.2",
    "husky": "^8.0.3",
    "pnpm": "^7.33.3",
    "prettier": "^2.8.8",
    "prettier-plugin-prisma": "^4.13.0",
    "rollup-plugin-typescript-paths": "^1.4.0",
    "sass": "^1.63.6",
    "typescript": "^5.1.6",
    "unplugin-auto-import": "^0.16.4",
    "unplugin-icons": "^0.16.3",
    "unplugin-vue-components": "^0.25.1",
    "vite": "^4.3.9",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vitest": "^0.29.8",
    "vue-tsc": "^1.8.3"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
