{
	"private": true,
	"packageManager": "pnpm@7.4.0",
	"scripts": {
		"build": "vite build",
		"dev": "concurrently -n front,back \"pnpm run start:front\" \"pnpm run start:back\"",
		"lint": "eslint --fix --cache .",
		"prepare": "husky install",
		"preview": "vite preview",
		"publish": "(cd api && pnpm run generate-route-types) && cp package.json package-real.json && mv package-publish.json package.json && pnpm publish --no-git-checks; mv package.json package-publish.json && mv package-real.json package.json",
		"start:back": "cd api && pnpm run dev",
		"start:front": "concurrently -n vite,vue-tsc \"vite --port 8001\" \"vue-tsc --noEmit --watch\"",
		"story:dev": "histoire dev",
		"test": "vitest",
		"typecheck": "vue-tsc --noEmit",
		"up": "taze major -I"
	},
	"dependencies": {
		"@bperel/vue3-popper-teleport": "^1.4.3",
		"@mapbox/mapbox-gl-geocoder": "^5.0.1",
		"@popperjs/core": "^2.11.7",
		"@sentry/tracing": "^7.48.0",
		"@sentry/vue": "^7.48.0",
		"@vueuse/core": "^9.13.0",
		"@vueuse/head": "^1.1.23",
		"axios": "^1.3.5",
		"axios-cache-interceptor": "^1.0.1",
		"bootstrap": "^5.2.3",
		"bootstrap-icons-vue": "^1.10.3",
		"bootstrap-vue-next": "^0.7.3",
		"chart.js": "^4.2.1",
		"concurrently": "^7.6.0",
		"core-js": "^3.30.1",
		"dayjs": "^1.11.7",
		"js-cookie": "^3.0.1",
		"js-yaml": "^4.1.0",
		"mapbox-gl": "^2.14.1",
		"page-flip": "^2.0.7",
		"pinia": "^2.0.34",
		"timeago.js": "^4.0.2",
		"v-contextmenu": "^3.0.0",
		"vue": "^3.2.47",
		"vue-chartjs": "^5.2.0",
		"vue-i18n": "^9.2.2",
		"vue-i18n-extract": "^2.0.7",
		"vue-mapbox-ts": "^0.9.7",
		"vue-router": "^4.1.6",
		"vue-slicksort": "^2.0.5"
	},
