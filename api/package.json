{
  "name": "dm-api",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "tsc && tsc-alias",
    "start": "ts-node -r tsconfig-paths/register index.ts",
    "dev": "nodemon",
    "prisma:pull": "ls prisma/*.prisma | while read -r schema; do pnpx prisma db pull --schema=$schema; done",
    "prisma:generate": "ls prisma/*.prisma | while read -r schema; do pnpx prisma generate --schema=$schema; done"
  },
  "dependencies": {
    "@prisma/client": "^4.3.1",
    "@pusher/push-notifications-server": "^1.2.5",
    "@sentry/node": "^7.12.0",
    "axios": "^0.27.2",
    "body-parser": "^1.20.0",
    "cloudinary": "^1.31.0",
    "connect-busboy": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "express-file-routing": "^2.0.3",
    "jsonwebtoken": "^8.5.1",
    "mariadb": "^3.0.1",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@swc/core": "^1.2.246",
    "@types/body-parser": "^1.19.2",
    "@types/connect-busboy": "^1.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.12",
    "@types/csurf": "^1.11.2",
    "@types/express": "^4.17.13",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^16.11.56",
    "prisma": "^4.3.1",
    "tsc-alias": "^1.7.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.2"
  }
}
