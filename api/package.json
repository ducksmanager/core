{
  "name": "dm-api",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rm -rf dist/api dist/types && tsc && tsc-alias",
    "start": "ts-node -r tsconfig-paths/register index.ts",
    "test": "which ts-node",
    "dev": "nodemon",
    "generate-route-types": "ts-node -r tsconfig-paths/register generate-route-types.ts && cd .. && node_modules/.bin/eslint --fix types/routes.ts",
    "prisma:pull": "ls prisma/*.prisma | while read -r schema; do pnpx prisma db pull --schema=$schema; done",
    "prisma:generate": "ls prisma/*.prisma | while read -r schema; do pnpx prisma generate --schema=$schema; done"
  },
  "dependencies": {
    "@prisma/client": "^4.13.0",
    "@pusher/push-notifications-server": "^1.2.6",
    "@sentry/node": "^7.50.0",
    "axios": "^1.4.0",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.36.4",
    "connect-busboy": "^1.0.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "csurf": "^1.11.0",
    "csv-parse": "^5.3.9",
    "dayjs": "^1.11.7",
    "dotenv": "^16.0.3",
    "ejs": "^3.1.9",
    "express": "^4.18.2",
    "express-file-routing": "^2.1.3",
    "i18n": "^0.15.1",
    "jsonwebtoken": "^9.0.0",
    "mariadb": "^3.1.2",
    "nodemailer": "^6.9.1",
    "nodemon": "^2.0.22",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.56",
    "@types/body-parser": "^1.19.2",
    "@types/busboy": "^1.5.0",
    "@types/connect-busboy": "^1.0.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/csurf": "^1.11.2",
    "@types/ejs": "^3.1.2",
    "@types/express": "^4.17.17",
    "@types/i18n": "^0.13.6",
    "@types/jsonwebtoken": "^9.0.2",
    "@types/node": "^16.18.25",
    "@types/nodemailer": "^6.4.7",
    "prisma": "^4.13.0",
    "tsc-alias": "^1.8.6",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.0.4"
  }
}
