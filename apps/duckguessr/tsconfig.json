{
  "extends": "./.nuxt/tsconfig.json",
  "compilerOptions": {
    "baseUrl": ".",
    "verbatimModuleSyntax": false,
    "module": "ESNext",
    "moduleResolution": "bundler",
    "paths": {
      "~/*": [
        "./app/*"
      ],
      "~dm-services/*": [
        "../../packages/api/services/*"
      ],
      "~duckguessr-services/*": [
        "./api/services/*"
      ],
      "~duckguessr-types/*": [
        "./api/types/*"
      ],
      "~duckguessr-prisma-browser": [
        "./api/prisma/client_duckguessr/browser"
      ],
      "~web": [
        "../web"
      ],
      "~web/*": [
        "../web/*"
      ]
    }
  },
  "include": [
    ".storybook/*.ts",
    "api/types/*.ts",
    "api/services/**/*.ts",
    "app/**/*.d.ts",
    "app/**/*.ts",
    "app/**/*.vue",
    ".nuxt/imports.d.ts",
    ".nuxt/components.d.ts",
    ".nuxt/nuxt.d.ts",
    "eslint.config.mjs",
    "nuxt.config.ts",
    "sentry.client.config.ts",
    "env.d.ts",
  ]
}