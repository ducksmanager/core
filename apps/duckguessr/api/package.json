{
  "name": "~duckguessr-api",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "type": "module",
  "version": "1.0.0",
  "description": "Duckguessr API",
  "main": "index.ts",
  "scripts": {
    "build": "rm -rf dist && ./node_modules/.bin/tsc && tsc-alias",
    "dev": "concurrently --kill-others-on-fail -n api,docker-compose \"tsx watch .\" \"docker compose -f docker-compose-dev.yml up\"",
    "lint": "eslint --fix --cache .",
    "prisma": "prisma db pull && prisma generate"
  },
