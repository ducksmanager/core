  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^5.7.0",
    "@sentry/node": "^7.84.0",
    "body-parser": "^1.20.2",
    "cookie": "^0.5.0",
    "dotenv": "^16.3.1",
    "esm": "^3.2.25",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "prisma": "^5.7.0",
    "sharp": "^0.33.0",
    "socket.io": "^4.7.2",
    "typescript": "^5.3.2"
  },
  "devDependencies": {
    "@swc/core": "^1.3.100",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/sharp": "^0.32.0",
    "@typescript-eslint/eslint-plugin": "^6.13.1",
    "@typescript-eslint/parser": "^6.13.1",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^3.14.0"
