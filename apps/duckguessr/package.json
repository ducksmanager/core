    "socket-call-client": "^0.7.5",
    "universal-cookie": "^4.0.4",
    "vue": "^3.4.27",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
