{
  "name": "~duckguessr",
  "type": "module",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "version": "1.0.0",
  "scripts": {
    "duckguessr": "concurrently -p \"[duckguessr-{name}]\" --kill-others-on-fail -n web,api,dm \"pnpm -F ~duckguessr dev\" \"pnpm -F ~duckguessr-api dev\" \"pnpm -F ~web web\"",
    "dev": "nuxt dev --port 8007 --host 0.0.0.0",
    "dev-storybook": "concurrently \"pnpm -F ~duckguessr-api dev\" \"storybook dev\"",
    "build": "nuxt build && pnpm build-storybook",
    "build-storybook": "storybook build",
    "generate": "nuxt generate",
    "preview": "nuxt preview",
    "postinstall": "nuxt prepare",
    "lint": "eslint --fix .",
    "storybook": "storybook dev -p 6006",
    "prod:deploy": "DIR=apps/duckguessr SERVICES='storybook' pnpm -F '~ci' prod:docker-compose-up",
    "prod:build-docker": "REPO_NAME=ghcr.io/bperel/duckguessr-storybook pnpm -F '~ci' prod:build-docker --target storybook -f apps/duckguessr/Dockerfile",
    "prod:transfer-files-pre": "pnpm -F '~ci' prod:transfer-files @apps/duckguessr/.env:apps/duckguessr/.env",
    "prod:transfer-files-post": "pnpm -F '~ci' prod:transfer-files apps/duckguessr/docker-compose.yml:@apps/duckguessr/docker-compose.yml apps/duckguessr/Caddyfile:@apps/duckguessr/Caddyfile"
  },
  "dependencies": {
    "@bootstrap-vue-next/nuxt": "^0.42.0",
    "@nuxtjs/i18n": "^10.2.1",
    "@nuxtjs/storybook": "^9.0.1",
    "@pinia/nuxt": "^0.11.3",
    "@sentry/nuxt": "^10.38.0",
    "@unhead/vue": "^2.1.2",
    "@vueuse/core": "^14.2.0",
    "@vueuse/integrations": "^14.2.0",
    "@vueuse/nuxt": "^14.2.0",
    "axios-cache-interceptor": "^1.11.4",
    "bootstrap": "^5.3.8",
    "bootstrap-vue-next": "^0.42.0",
    "js-cookie": "^3.0.5",
    "nuxt": "^4.3.0",
    "pinia": "^3.0.4",
    "socket-call-client": "^0.7.7",
    "universal-cookie": "^8.0.1",
    "unplugin-vue-components": "^31.0.0",
    "vue": "^3.5.27",
    "~api": "workspace:*",
    "~dm-types": "workspace:*",
    "~duckguessr-api": "workspace:*",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@iconify-json/bi": "^1.2.7",
    "@nuxt/devtools": "^3.1.1",
    "@nuxt/eslint": "^1.13.0",
    "@nuxt/typescript-build": "^3.0.2",
    "@storybook-vue/nuxt": "^9.0.1",
    "@storybook/vue3": "^9.1.17",
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^8.4.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.19.7",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^10.2.3",
    "eslint-plugin-unused-imports": "^4.3.0",
    "eslint-plugin-vue": "^10.7.0",
    "globals": "^16.5.0",
    "playwright": "^1.58.1",
    "prettier": "^3.8.1",
    "sass": "^1.97.3",
    "storybook": "^10.2.3",
    "typescript": "^5.9.3",
    "typescript-eslint": "^8.54.0",
    "unplugin-icons": "^22.5.0",
    "vite-plugin-checker": "^0.12.0",
    "vue-tsc": "3.1.5"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}