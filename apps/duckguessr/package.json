{
  "name": "~duckguessr",
  "type": "module",
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "version": "1.0.0",
  "scripts": {
    "duckguessr": "concurrently -p \"[duckguessr-{name}]\" --kill-others-on-fail -n web,api,dm \"pnpm -F ~duckguessr dev\" \"pnpm -F ~duckguessr-api dev\" \"pnpm -F ~web web\"",
    "dev": "concurrently -n vite,vue-tsc \"vite --port 8007\" \"vue-tsc --noEmit --watch\"",
    "dev-storybook": "concurrently \"pnpm -F ~duckguessr-api dev\" \"storybook dev\"",
    "build": "storybook build",
    "build-storybook": "storybook build",
    "lint": "eslint --fix .",
    "storybook": "storybook dev -p 6006",
    "prod:deploy": "DIR=apps/duckguessr SERVICES='storybook' pnpm -F '~ci' prod:docker-compose-up",
    "prod:build-docker": "REPO_NAME=ghcr.io/bperel/duckguessr-storybook pnpm -F '~ci' prod:build-docker --target storybook -f apps/duckguessr/Dockerfile",
    "prod:transfer-files-pre": "pnpm -F '~ci' prod:transfer-files @apps/duckguessr/.env:apps/duckguessr/.env",
    "prod:transfer-files-post": "pnpm -F '~ci' prod:transfer-files apps/duckguessr/docker-compose.yml:@apps/duckguessr/docker-compose.yml apps/duckguessr/Caddyfile:@apps/duckguessr/Caddyfile"
  },
  "dependencies": {
    "@prisma/client": "^6.14.0",
    "@sentry/browser": "^9.46.0",
    "@sentry/tracing": "^7.120.4",
    "@sentry/vue": "^9.46.0",
    "@unhead/vue": "^2.0.14",
    "@vueuse/core": "^13.9.0",
    "@vueuse/integrations": "^13.9.0",
    "bootstrap": "^5.3.7",
    "bootstrap-vue-next": "^0.30.5",
    "js-cookie": "^3.0.5",
    "pinia": "^3.0.3",
    "socket-call-client": "^0.7.6",
    "universal-cookie": "^8.0.1",
    "vue": "^3.5.21",
    "vue-i18n": "^9.14.5",
    "vue-router": "^4.5.1",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.33.0",
    "@iconify-json/bi": "^1.2.6",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@storybook/vue3": "^9.1.5",
    "@storybook/vue3-vite": "^9.1.5",
    "@types/eslint": "^9.6.1",
    "@types/eslint-scope": "^8.3.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.17.2",
    "@vitejs/plugin-vue": "^6.0.1",
    "concurrently": "^9.2.1",
    "eslint": "^9.35.0",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-prettier": "^5.5.4",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "eslint-plugin-storybook": "^9.1.5",
    "eslint-plugin-unused-imports": "^4.2.0",
    "eslint-plugin-vue": "^10.4.0",
    "globals": "^16.3.0",
    "playwright": "^1.54.2",
    "prettier": "^3.6.2",
    "sass": "^1.90.0",
    "storybook": "^9.1.5",
    "typescript": "^5.9.2",
    "typescript-eslint": "^8.42.0",
    "unplugin-auto-import": "^19.3.0",
    "unplugin-icons": "^22.2.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.1.5",
    "vite-plugin-pages": "^0.33.1",
    "vite-plugin-static-copy": "^3.1.2",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^3.0.6"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}