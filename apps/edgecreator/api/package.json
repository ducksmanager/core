{
  "name": "dm-api",
  "version": "1.0.0",
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rm -rf dist/api dist/types && tsc && tsc-alias",
    "start": "ts-node -r tsconfig-paths/register index.ts",
    "test": "which ts-node",
    "dev": "nodemon",
    "generate-route-types": "ts-node -r tsconfig-paths/register generate-route-types.ts && cd .. && node_modules/.bin/eslint --fix types/routes.ts"
  },
  "dependencies": {
    "@sentry/node": "^7.39.0",
    "axios": "^1.3.4",
    "body-parser": "^1.20.2",
    "cloudinary": "^1.34.0",
    "cookie-parser": "^1.4.6",
    "cors": "^2.8.5",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "express-file-routing": "^2.1.2",
    "i18n": "^0.15.1",
    "image-size": "^1.0.2",
    "jsonwebtoken": "^9.0.0",
    "multer": "1.4.5-lts.1",
    "node-base64-image": "^2.0.4",
    "nodemon": "^2.0.20",
    "sharp": "^0.31.3",
    "ts-node": "^10.9.1"
  },
  "devDependencies": {
    "@swc/core": "^1.3.37",
    "@types/body-parser": "^1.19.2",
    "@types/busboy": "^1.5.0",
    "@types/cookie-parser": "^1.4.3",
    "@types/cors": "^2.8.13",
    "@types/express": "^4.17.17",
    "@types/i18n": "^0.13.6",
    "@types/jsonwebtoken": "^9.0.1",
    "@types/multer": "^1.4.7",
    "@types/node": "^16.18.13",
    "@types/sharp": "^0.31.1",
    "tsc-alias": "^1.8.2",
    "tsconfig-paths": "^4.1.2",
    "typescript": "^4.9.5"
  }
}
