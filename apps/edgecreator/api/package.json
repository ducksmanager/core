    "@rollup/plugin-typescript": "^11.1.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/i18n": "^0.13.12",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/multer": "^1.4.11",
    "@types/node": "^18.19.33",
    "concurrently": "^8.2.2",
    "rollup-plugin-node-globals": "^1.4.0",
    "rollup-plugin-tsc-alias": "^1.1.2",
    "tsc-alias": "^1.8.9",
    "tsx": "^4.10.1",
    "typescript": "^5.4.5"
