{
  "name": "edgecreator",
  "private": true,
  "packageManager": "pnpm@7.4.0",
  "scripts": {
    "build": "vite build",
    "dev": "concurrently -n front,back \"pnpm run start:front\" \"pnpm run start:back\"",
    "lint": "eslint --fix --cache .",
    "prepare": "husky install",
    "preview": "vite preview",
    "start:back": "cd api && pnpm run dev",
    "start:front": "concurrently -n vite,vue-tsc \"vite --port 8001\" \"vue-tsc --noEmit --watch\"",
    "typecheck": "vue-tsc --noEmit"
  },
  "dependencies": {
    "@bperel/vue3-popper-teleport": "^1.4.3",
    "@popperjs/core": "^2.11.7",
    "@uppy/core": "^1.20.1",
    "@uppy/dashboard": "^1.21.1",
    "@uppy/drag-drop": "^1.4.31",
    "@uppy/locales": "^1.22.0",
    "@uppy/status-bar": "^1.9.6",
    "@uppy/url": "^1.5.23",
    "@uppy/webcam": "^1.8.13",
    "@uppy/xhr-upload": "^1.7.5",
    "@vueuse/core": "^9.13.0",
    "@vueuse/head": "^1.1.23",
    "@vueuse/integrations": "^9.13.0",
    "axios": "^1.3.5",
    "axios-cache-interceptor": "^1.0.1",
    "bootstrap": "^5.2.3",
    "bootstrap-vue-next": "^0.8.4",
    "cropperjs": "^1.5.13",
    "ducksmanager": "^0.0.47",
    "interactjs": "^1.10.17",
    "pinia": "^2.0.34",
    "sass": "^1.62.0",
    "universal-cookie": "^4.0.4",
    "vue": "^3.2.47",
    "vue-cropperjs": "^5.0.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.1.6",
    "vue3-simple-typeahead": "^1.0.11",
    "xmldom": "^0.6.0"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.1.16",
    "@intlify/unplugin-vue-i18n": "^0.10.0",
    "@types/js-cookie": "^3.0.3",
    "@types/vue-cropperjs": "^4.1.2",
    "@types/xmldom": "^0.1.31",
    "@typescript-eslint/eslint-plugin": "^5.58.0",
    "@typescript-eslint/parser": "^5.58.0",
    "@vitejs/plugin-vue": "^4.1.0",
    "concurrently": "^8.0.1",
    "eslint": "^8.38.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^2.0.0",
    "eslint-plugin-vue": "^9.10.0",
    "husky": "^8.0.3",
    "prettier": "^2.8.7",
    "typescript": "^4.9.5",
    "unplugin-auto-import": "^0.15.2",
    "unplugin-icons": "^0.16.1",
    "unplugin-vue-components": "^0.24.1",
    "vite": "^4.2.1",
    "vite-plugin-pages": "^0.29.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vue-tsc": "^1.2.0"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
