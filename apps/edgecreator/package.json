{
  "name": "~edgecreator",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "scripts": {
    "build": "mv .env.prod .env && vite build",
    "dev": "VITE_EDGES_PATH=`realpath ../edges/edges` concurrently -n vite,vue-tsc \"vite --port 8002\" \"vue-tsc --noEmit --watch\"",
    "lint": "eslint --fix .",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "prod:deploy": "SERVICES=app-websocket DIR=apps/edgecreator pnpm -F '~ci' prod:docker-compose-up",
    "prod:build-docker": "REPO_NAME=ghcr.io/bperel/edgecreator-app:websocket pnpm -F '~ci' prod:build-docker -f apps/edgecreator/Dockerfile",
    "prod:transfer-files-post": "pnpm -F '~ci' prod:transfer-files apps/edgecreator/docker-compose.yml:@apps/edgecreator/docker-compose.yml apps/edgecreator/config/sentry-relay/config.yml:@apps/edgecreator/config/sentry-relay/config.yml"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@unhead/vue": "^1.11.14",
    "@uppy/core": "^1.20.1",
    "@uppy/dashboard": "^1.21.1",
    "@uppy/drag-drop": "^1.4.31",
    "@uppy/locales": "^1.22.0",
    "@uppy/status-bar": "^1.9.6",
    "@uppy/url": "^1.5.23",
    "@uppy/webcam": "^1.8.13",
    "@uppy/xhr-upload": "^1.7.5",
    "@vueuse/core": "^12.2.0",
    "@vueuse/integrations": "^12.2.0",
    "axios": "^1.7.9",
    "axios-cache-interceptor": "^1.6.2",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.26.18",
    "cropperjs": "^1.6.2",
    "interactjs": "^1.10.27",
    "js-cookie": "^3.0.5",
    "pinia": "^2.3.0",
    "sass": "^1.83.0",
    "universal-cookie": "^7.2.2",
    "vue": "^3.5.13",
    "vue-cropperjs": "^5.0.0",
    "vue-i18n": "^9.14.2",
    "vue-router": "^4.5.0",
    "vue3-simple-typeahead": "^1.0.11",
    "~edgecreator-api": "workspace:*",
    "~edges": "workspace:*",
    "~prisma-schemas": "workspace:*",
    "~socket.io-client-services": "workspace:*",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.17.0",
    "@iconify-json/bi": "^1.2.2",
    "@intlify/unplugin-vue-i18n": "^5.3.1",
    "@rushstack/eslint-patch": "^1.10.4",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.10.2",
    "@types/vue-cropperjs": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.18.2",
    "@vitejs/plugin-vue": "^5.2.1",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^14.2.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.17.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^9.32.0",
    "prettier": "^3.4.2",
    "typescript": "^5.7.2",
    "unplugin-auto-import": "^0.19.0",
    "unplugin-icons": "^0.21.0",
    "unplugin-vue-components": "^0.28.0",
    "vite": "^6.0.6",
    "vite-plugin-pages": "^0.32.4",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^2.2.0"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}