{
  "name": "~edgecreator",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "build": "vite build",
    "dev": "concurrently -n vite,vue-tsc \"vite --port 8002\" \"vue-tsc --noEmit --watch\"",
    "lint": "eslint --fix --cache .",
    "typecheck": "vue-tsc --noEmit",
    "prod:deploy": "DIR=apps/edgecreator pnpm -F '~ci' prod:docker-compose-up",
    "prod:build-docker": "REPO_NAME=ghcr.io/bperel/edgecreator-app pnpm -F '~ci' prod:build-docker -f apps/edgecreator/Dockerfile",
    "prod:transfer-files-pre": "pnpm -F '~ci' prod:transfer-files @apps/edgecreator/.env:apps/edgecreator/.env @packages/prisma-schemas/.env.prod.local:packages/prisma-schemas/.env",
    "prod:transfer-files-post": "pnpm -F '~ci' prod:transfer-files apps/edgecreator/docker-compose.yml:@apps/edgecreator/docker-compose.yml apps/edgecreator/config/sentry-relay/config.yml:@apps/edgecreator/config/sentry-relay/config.yml"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@unhead/vue": "^1.9.10",
    "@uppy/core": "^1.20.1",
    "@uppy/dashboard": "^1.21.1",
    "@uppy/drag-drop": "^1.4.31",
    "@uppy/locales": "^1.22.0",
    "@uppy/status-bar": "^1.9.6",
    "@uppy/url": "^1.5.23",
    "@uppy/webcam": "^1.8.13",
    "@uppy/xhr-upload": "^1.7.5",
    "@vueuse/core": "^10.9.0",
    "@vueuse/integrations": "^10.9.0",
    "axios": "^1.6.8",
    "axios-cache-interceptor": "^1.5.2",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.22.0",
    "cropperjs": "^1.6.2",
    "interactjs": "^1.10.27",
    "pinia": "^2.1.7",
    "sass": "^1.77.1",
    "universal-cookie": "^4.0.4",
    "vue": "^3.4.27",
    "vue-cropperjs": "^5.0.0",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.3.2",
    "vue3-simple-typeahead": "^1.0.11",
    "xmldom": "^0.6.0",
    "~api-routes": "workspace:*",
    "~axios-helper": "workspace:*",
    "~dm-types": "workspace:*",
    "~prisma-schemas": "workspace:*"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.1.23",
    "@intlify/unplugin-vue-i18n": "^0.11.0",
    "@rushstack/eslint-patch": "^1.10.2",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^18.0.0",
    "@types/vue-cropperjs": "^4.1.6",
    "@types/xmldom": "^0.1.34",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-prettier": "^7.1.0",
    "@vue/eslint-config-typescript": "^11.0.3",
    "concurrently": "^8.2.2",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-vue": "^9.26.0",
    "prettier": "^3.2.5",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.17.6",
    "unplugin-icons": "^0.19.0",
    "unplugin-vue-components": "^0.27.0",
    "vite": "^5.2.11",
    "vite-plugin-pages": "^0.32.1",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^2.0.21"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}