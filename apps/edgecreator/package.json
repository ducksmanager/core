{
  "name": "~edgecreator",
  "type": "module",
  "files": [
    "dist"
  ],
  "engines": {
    "node": ">=22.0.0 <23.0.0"
  },
  "scripts": {
    "build": "vite build",
    "dev": "VITE_EDGES_PATH=`realpath ../edges/edges` concurrently -n vite,vue-tsc \"vite --port 8002\" \"vue-tsc --noEmit --watch\"",
    "lint": "eslint --fix .",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "prod:deploy": "SERVICES=app-websocket DIR=apps/edgecreator pnpm -F '~ci' prod:docker-compose-up",
    "prod:build-docker": "REPO_NAME=ghcr.io/bperel/edgecreator-app:websocket pnpm -F '~ci' prod:build-docker -f apps/edgecreator/Dockerfile",
    "prod:transfer-files-pre": "pnpm -F '~ci' prod:transfer-files @apps/edgecreator/.env:apps/edgecreator/.env",
    "prod:transfer-files-post": "pnpm -F '~ci' prod:transfer-files apps/edgecreator/docker-compose.yml:@apps/edgecreator/docker-compose.yml apps/edgecreator/config/sentry-relay/config.yml:@apps/edgecreator/config/sentry-relay/config.yml apps/edgecreator/Caddyfile:@apps/edgecreator/Caddyfile"
  },
  "dependencies": {
    "@popperjs/core": "^2.11.8",
    "@unhead/vue": "^1.11.20",
    "@uppy/core": "^1.20.1",
    "@uppy/dashboard": "^1.21.1",
    "@uppy/drag-drop": "^1.4.31",
    "@uppy/locales": "^1.22.0",
    "@uppy/status-bar": "^1.9.6",
    "@uppy/url": "^1.5.23",
    "@uppy/webcam": "^1.8.13",
    "@uppy/xhr-upload": "^1.7.5",
    "@vue-macros/reactivity-transform": "^3.1.2",
    "@vueuse/core": "^13.9.0",
    "@vueuse/integrations": "^13.9.0",
    "axios": "^1.13.2",
    "axios-cache-interceptor": "^1.11.3",
    "bootstrap": "^5.3.8",
    "bootstrap-vue-next": "^0.42.0",
    "cropperjs": "^1.6.2",
    "interactjs": "^1.10.27",
    "js-cookie": "^3.0.5",
    "pinia": "^3.0.4",
    "sass": "^1.97.2",
    "socket-call-client": "^0.7.7",
    "universal-cookie": "^7.2.2",
    "vite-multiple-assets": "^2.2.6",
    "vue": "^3.5.26",
    "vue-cropperjs": "^5.0.0",
    "vue-i18n": "^11.2.8",
    "vue-router": "^4.6.4",
    "vue3-simple-typeahead": "^1.0.11",
    "~edgecreator-api": "workspace:*",
    "~edges": "workspace:*",
    "~group-by": "workspace:*",
    "~prisma-schemas": "workspace:*",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@eslint/compat": "^2.0.1",
    "@eslint/eslintrc": "^3.3.3",
    "@eslint/js": "^9.39.2",
    "@iconify-json/bi": "^1.2.7",
    "@intlify/unplugin-vue-i18n": "^6.0.8",
    "@rushstack/eslint-patch": "^1.15.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^22.19.5",
    "@types/vue-cropperjs": "^4.1.6",
    "@typescript-eslint/eslint-plugin": "^8.52.0",
    "@vitejs/plugin-vue": "^6.0.3",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "concurrently": "^9.2.1",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^10.6.2",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "unplugin-auto-import": "^20.3.0",
    "unplugin-icons": "^22.5.0",
    "unplugin-vue-components": "^28.8.0",
    "vite": "^7.3.1",
    "vite-plugin-pages": "^0.33.2",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "3.1.5"
  }
}