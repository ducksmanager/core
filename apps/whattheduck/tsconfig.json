{
  "compilerOptions": {
    "target": "ESNext",
    "useDefineForClassFields": true,
    "module": "ESNext",
    "moduleResolution": "Node",
    "strict": true,
    "jsx": "preserve",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "esModuleInterop": true,
    "lib": ["ESNext", "DOM"],
    "skipLibCheck": true,
    "noEmit": true,
    "paths": {
      "~/*": ["./src/*"],
      "~types/*": [
        "./types/*"
      ],
      "~dm_types/*": [
        "./node_modules/ducksmanager/types/*"
      ],
      "~prisma_clients/*": [
        "./node_modules/ducksmanager/api/dist/prisma/*"
      ]
    }
  },
  "include": ["src/**/*.ts", "src/**/*.d.ts", "src/**/*.tsx", "src/**/*.vue"],
  "references": [{ "path": "./tsconfig.node.json" }],
  "types": [
    "vite/client",
    "vue/ref-macros"
  ]
}
