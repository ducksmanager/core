    "@histoire/plugin-vue": "^0.17.17",
    "@ionic/prettier-config": "^4.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/node": "^18.19.43",
    "@vitejs/plugin-vue": "^5.1.2",
    "@vue-macros/reactivity-transform": "^1.0.3",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.6",
