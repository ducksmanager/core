    "@capacitor/camera": "^6.0.2",
    "@capacitor/clipboard": "^6.0.1",
    "@capacitor/core": "^6.1.2",
    "@capacitor/haptics": "^6.0.1",
    "@capacitor/ios": "^6.1.2",
    "@capacitor/keyboard": "^6.0.2",
    "@capacitor/preferences": "^6.0.2",
    "@capacitor/status-bar": "^6.0.1",
    "@capawesome/capacitor-file-picker": "^6.0.1",
    "@capgo/capacitor-updater": "^6.0.56",
    "@ionic/pwa-elements": "^3.3.0",
    "@ionic/storage": "^4.0.0",
    "@ionic/vue": "^8.2.6",
    "@ionic/vue-router": "^8.2.6",
    "@nanoandrew4/vue3-carousel-3d": "^1.0.0",
    "@vueuse/components": "^10.11.1",
    "@vueuse/core": "^10.11.1",
    "@vueuse/integrations": "^10.11.1",
    "chart.js": "^4.4.3",
    "dayjs": "^1.11.12",
    "ionicons": "^7.4.0",
    "js-cookie": "^3.0.5",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "pinia": "^2.2.1",
    "sass": "^1.77.8",
    "universal-cookie": "^7.2.0",
    "vue": "^3.4.37",
    "vue-chartjs": "^5.3.1",
    "vue-i18n": "^9.13.1",
    "vue-router": "^4.4.3",
    "vue-virtual-scroller": "2.0.0-beta.8",
    "~dm-types": "workspace:*",
    "~prisma-schemas": "workspace:*",
    "~socket.io-client-services": "workspace:*",
    "~socket.io-services": "workspace:*",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@capacitor/assets": "^3.0.5",
