{
  "name": "~whattheduck-ionic",
  "version": "0.0.1",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "whattheduck": "concurrently -p \"[whattheduck-{name}]\" --kill-others-on-fail -n whattheduck-ionic,api \"pnpm -F ~whattheduck-ionic... -F !~web -F !~prisma-clients dev\" \"pnpm -F ~api... dev\"",
    "lint": "eslint --fix .",
    "dev": "vite dev --host",
    "build": "vue-tsc && vite build",
    "ionic:build": "npm run build",
    "preview": "vite preview",
    "test:e2e": "cypress run",
    "test:unit": "vitest"
  },
  "dependencies": {
    "@capacitor-community/camera-preview": "^5.0.0",
    "@capacitor-community/sqlite": "5.4.2-2",
    "@capacitor/android": "^5.5.1",
    "@capacitor/app": "^5.0.6",
    "@capacitor/camera": "^5.0.7",
    "@capacitor/core": "^5.5.1",
    "@capacitor/haptics": "^5.0.6",
    "@capacitor/ios": "^5.5.1",
    "@capacitor/keyboard": "^5.0.6",
    "@capacitor/preferences": "^5.0.6",
    "@capacitor/splash-screen": "^5.0.6",
    "@capacitor/status-bar": "^5.0.6",
    "@capawesome/capacitor-file-picker": "^5.1.1",
    "@ionic/pwa-elements": "^3.2.2",
    "@ionic/storage": "^4.0.0",
    "@ionic/vue": "^7.5.4",
    "@ionic/vue-router": "^7.5.4",
    "@vueuse/integrations": "^10.6.0",
    "axios": "^1.6.1",
    "axios-cache-interceptor": "^1.3.2",
    "chart.js": "^4.4.0",
    "dayjs": "^1.11.10",
    "ionicons": "^7.2.1",
    "jeep-sqlite": "^2.5.4",
    "localforage-cordovasqlitedriver": "^1.8.0",
    "pinia": "^2.1.7",
    "reflect-metadata": "^0.1.13",
    "sass": "^1.69.5",
    "sass-loader": "^13.3.2",
    "typeorm": "^0.3.17",
    "universal-cookie": "^6.1.1",
    "vue": "^3.3.8",
    "vue-chartjs": "^5.2.0",
    "vue-i18n": "^9.6.5",
    "vue-router": "^4.2.5",
    "~api-routes": "workspace:^",
    "~axios-helper": "workspace:^",
    "~dm-types": "workspace:*",
    "~prisma-clients": "workspace:*",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@capacitor/cli": "^5.5.1",
    "@ionic/prettier-config": "^4.0.0",
    "@types/node": "^16.18.61",
    "@typescript-eslint/eslint-plugin": "^6.10.0",
    "@typescript-eslint/parser": "^6.10.0",
    "@typescript-eslint/typescript-estree": "^6.10.0",
    "@vitejs/plugin-vue": "^4.4.1",
    "@vue/eslint-config-typescript": "^12.0.0",
    "@vue/test-utils": "^2.4.1",
    "concurrently": "^7.1.0",
    "cypress": "^13.5.0",
    "eslint": "^8.53.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^3.6.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "jsdom": "^21.1.2",
    "prettier": "^3.0.3",
    "tslib": "^2.6.2",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.22"
  },
  "description": "What The Duck",
  "prettier": "@ionic/prettier-config"
}