    "concurrently": "^8.2.2",
    "ejs-lint": "^2.0.0",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.0.0",
    "eslint-plugin-vue": "^9.20.1",
    "histoire": "^0.17.9",
    "prettier": "^3.2.4",
    "prettier-plugin-prisma": "^5.0.0",
