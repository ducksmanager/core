{
  "name": "~web",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "web": "concurrently -p \"[dm-{name}]\" --kill-others-on-fail -n front,api \"pnpm -F ~web dev\" \"pnpm -F ~api dev\"",
    "build": "vite build",
    "lint": "eslint --fix --cache .",
    "preview": "vite preview",
    "dev": "concurrently -n vite,vue-tsc \"vite --port 8001\" \"vue-tsc --noEmit --watch\"",
    "story:dev": "histoire dev",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "up": "taze major -I"
  },
  "dependencies": {
    "@bperel/vue3-popper-teleport": "^1.4.3",
    "@mapbox/mapbox-gl-geocoder": "^5.0.2",
    "@popperjs/core": "^2.11.8",
    "@sentry/tracing": "^7.98.0",
    "@sentry/vue": "^7.98.0",
    "@unhead/vue": "^1.8.10",
    "@vueuse/core": "^10.7.2",
    "axios-cache-interceptor": "^1.5.1",
    "bootstrap": "^5.3.2",
    "bootstrap-vue-next": "^0.15.5",
    "chart.js": "^4.4.1",
    "dayjs": "^1.11.10",
    "js-cookie": "^3.0.5",
    "page-flip": "^2.0.7",
    "pinia": "^2.1.7",
    "socket.io-client": "^4.7.4",
    "timeago.js": "^4.0.2",
    "v-contextmenu": "^3.1.1",
    "vue": "^3.4.15",
    "vue-chartjs": "^5.3.0",
    "vue-i18n": "^9.9.0",
    "vue-mapbox-ts": "^0.9.8",
    "vue-router": "^4.2.5",
    "vue-slicksort": "^2.0.5",
    "~api": "workspace:*",
    "~socket.io-services": "workspace:*",
    "~dm-types": "workspace:*",
    "~prisma-clients": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.2.1",
    "@histoire/plugin-vue": "^0.17.9",
    "@iconify-json/bi": "^1.1.23",
    "@intlify/unplugin-vue-i18n": "^2.0.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/js-cookie": "^3.0.6",
    "@types/mapbox__mapbox-gl-geocoder": "^4.7.7",
    "@types/page-flip": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^6.19.1",
    "@typescript-eslint/parser": "^6.19.1",
