{
  "name": "~web",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "web": "concurrently -p \"[dm-{name}]\" --kill-others-on-fail -n front,api \"pnpm -F ~web dev\" \"pnpm -F ~api dev\"",
    "build": "vite build",
    "lint": "eslint --fix --cache .",
    "preview": "vite preview",
    "dev": "concurrently -n vite,vue-tsc \"vite --port 8001\" \"vue-tsc --noEmit --watch\"",
    "start": "node index.js",
    "story:dev": "histoire dev",
    "test": "vitest",
    "typecheck": "vue-tsc --noEmit",
    "wait": "wait-on tcp:8001",
    "prod:deploy": "DIR=. SERVICES='web' pnpm -F '~ci' prod:docker-compose-up",
    "prod:build-docker": "REPO_NAME=ghcr.io/bperel/ducksmanager-web pnpm -F '~ci' prod:build-docker -f apps/web/Dockerfile",
    "prod:transfer-files-pre": "pnpm -F '~ci' prod:transfer-files @apps/web/.env.prod.legacy:apps/web/.env @packages/prisma-clients/.env.prod.local:packages/prisma-clients/.env",
    "prod:transfer-files-post": "pnpm -F '~ci' prod:transfer-files apps/web/docker-compose.yml:@docker-compose.yml"
  },
  "dependencies": {
    "@bperel/vue3-popper-teleport": "^1.4.3",
    "@mapbox/mapbox-gl-geocoder": "^5.0.2",
    "@popperjs/core": "^2.11.8",
    "@sentry/tracing": "^7.114.0",
    "@sentry/vue": "^7.116.0",
    "@unhead/vue": "^1.9.11",
    "@vueuse/core": "^10.9.0",
    "axios": "^1.7.2",
    "axios-cache-interceptor": "^1.5.3",
    "bootstrap": "^5.3.3",
    "bootstrap-icons-vue": "^1.11.3",
    "bootstrap-vue-next": "^0.14.10",
    "chart.js": "^4.4.3",
    "core-js": "^3.37.1",
    "dayjs": "^1.11.11",
    "js-cookie": "^3.0.5",
    "js-yaml": "^4.1.0",
    "mapbox-gl": "^2.15.0",
    "page-flip": "^2.0.7",
    "pinia": "^2.1.7",
    "timeago.js": "^4.0.2",
    "v-contextmenu": "^3.2.0",
    "vue": "^3.4.27",
    "vue-chartjs": "^5.3.1",
    "vue-i18n": "^9.13.1",
    "vue-i18n-extract": "^2.0.7",
    "vue-mapbox-ts": "^0.9.10",
    "vue-router": "^4.3.2",
    "vue-slicksort": "^2.0.5",
    "~api-routes": "workspace:*",
    "~axios-helper": "workspace:*",
    "~dm-types": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.2.1",
    "@histoire/plugin-vue": "^0.17.17",
    "@iconify-json/bi": "^1.1.23",
    "@intlify/unplugin-vue-i18n": "^1.6.0",
    "@rollup/plugin-typescript": "^11.1.6",
    "@types/js-cookie": "^3.0.6",
    "@types/mapbox__mapbox-gl-geocoder": "^4.7.7",
    "@types/page-flip": "^2.0.3",
    "@typescript-eslint/eslint-plugin": "^6.21.0",
    "@typescript-eslint/parser": "^6.21.0",
    "@vitejs/plugin-vue": "^4.6.2",
    "@vue-macros/reactivity-transform": "^0.4.5",
    "@vue/compiler-sfc": "^3.4.27",
    "@vue/test-utils": "^2.4.6",
    "concurrently": "^8.2.2",
    "ejs-lint": "^2.0.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-unused-imports": "^3.2.0",
    "eslint-plugin-vue": "^9.26.0",
    "histoire": "^0.17.17",
    "pnpm": "^8.15.8",
    "prettier": "^3.2.5",
    "prettier-plugin-prisma": "^5.0.0",
    "rollup-plugin-typescript-paths": "^1.5.0",
    "sass": "^1.77.2",
    "typescript": "^5.4.5",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-icons": "^0.17.4",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.3",
    "vite-plugin-pages": "^0.31.0",
    "vite-plugin-vue-layouts": "^0.8.0",
    "vitest": "^0.34.6",
    "vue-tsc": "^1.8.27"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}