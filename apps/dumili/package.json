{
  "name": "~dumili",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "dumili": "concurrently -p \"[dumili-{name}]\" --kill-others-on-fail -n front,api,dm,kumiko+paddleocr \"pnpm -F ~dumili dev\" \"pnpm -F ~dumili-api dev\" \"pnpm -F ~web web\" \"docker compose -f docker-compose-dev.yml up kumiko db\"",
    "dev": "concurrently -n vite,vue-tsc \"vite --port 8004\" \"vue-tsc --noEmit --watch\"",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "prod:deploy": "DIR=apps/dumili pnpm -F '~ci' prod:docker-compose-up",
    "prod-build-docker-web": "REPO_NAME=ghcr.io/bperel/dumili-app pnpm -F '~ci' prod:build-docker -f apps/dumili/Dockerfile --target web",
    "prod-build-docker-api": "REPO_NAME=ghcr.io/bperel/dumili-api pnpm -F '~ci' prod:build-docker -f apps/dumili/Dockerfile --target api",
    "prod-build-docker-kumiko": "REPO_NAME=ghcr.io/bperel/kumiko pnpm -F '~ci' prod:build-docker -f apps/dumili/kumiko/Dockerfile",
    "prod-build-docker-paddleocr": "REPO_NAME=ghcr.io/bperel/paddleocr pnpm -F '~ci' prod:build-docker -f apps/dumili/paddleocr/Dockerfile",
    "prod:build-docker": "pnpm run '/^prod-build-docker-/'",
    "prod:transfer-files-pre": "pnpm -F '~ci' prod:transfer-files @apps/dumili/api/.env.prod.local:apps/dumili/api/.env @apps/web/.env.prod.local:apps/web/.env.prod.local",
    "prod:transfer-files-post": "pnpm -F '~ci' prod:transfer-files apps/dumili/docker-compose.yml:@apps/dumili/docker-compose.yml"
  },
  "dependencies": {
    "@uppy/core": "^3.13.1",
    "@uppy/dashboard": "^3.9.1",
    "@uppy/drag-drop": "^3.1.1",
    "@uppy/progress-bar": "^3.1.1",
    "@uppy/xhr-upload": "^3.6.8",
    "@vueuse/core": "^11.1.0",
    "@vueuse/integrations": "^11.1.0",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.24.21",
    "dayjs": "^1.11.13",
    "js-cookie": "^3.0.5",
    "page-flip": "^2.0.7",
    "pinia": "^2.2.2",
    "vue": "^3.5.8",
    "vue-draggable-resizable": "^3.0.0",
    "vue-i18n": "^9.14.0",
    "vue-router": "^4.4.5",
    "vue-slicksort": "^2.0.5",
    "~socket.io-client-services": "workspace:*",
    "~socket.io-services": "workspace:*",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.27.3",
    "@iconify-json/bi": "^1.2.0",
    "@intlify/unplugin-vue-i18n": "^4.0.0",
    "@types/js-cookie": "^3.0.6",
    "@types/page-flip": "^2.0.3",
    "@vitejs/plugin-vue": "^5.1.4",
    "concurrently": "^9.0.1",
    "eslint": "^8.57.1",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^9.28.0",
    "prettier": "^3.3.3",
    "sass": "^1.79.3",
    "typescript": "^5.6.2",
    "typescript-eslint": "^7.18.0",
    "unplugin-auto-import": "^0.18.3",
    "unplugin-icons": "^0.19.3",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.7",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pages": "^0.32.3",
    "vue-tsc": "^2.1.6"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}