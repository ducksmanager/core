{
  "name": "~dumili",
  "version": "0.0.0",
  "type": "module",
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "dumili": "concurrently -p \"[dumili-{name}]\" --kill-others-on-fail -n front,api,dm,kumiko+paddleocr \"pnpm -F ~dumili dev\" \"pnpm -F ~dumili-api dev\" \"pnpm -F ~web web\" \"docker compose -f docker-compose-dev.yml up kumiko db\"",
    "dev": "concurrently -n vite,vue-tsc \"vite --port 8004\" \"vue-tsc --noEmit --watch\"",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint .",
    "prod:deploy": "DIR=apps/dumili pnpm -F '~ci' prod:docker-compose-up",
    "prod-build-docker-web": "REPO_NAME=ghcr.io/bperel/dumili-app pnpm -F '~ci' prod:build-docker -f apps/dumili/Dockerfile",
    "prod:build-docker": "pnpm run '/^prod-build-docker-/'",
    "prod:transfer-files-pre": "pnpm -F '~ci' prod:transfer-files @apps/web/.env.prod.local:apps/web/.env.prod.local",
    "prod:transfer-files-post": "pnpm -F '~ci' prod:transfer-files apps/dumili/docker-compose.yml:@apps/dumili/docker-compose.yml"
  },
  "dependencies": {
    "@iconify-json/bi": "^1.2.1",
    "@sentry/browser": "^8.39.0",
    "@sentry/vue": "^8.39.0",
    "@unhead/vue": "^1.11.11",
    "@vueuse/core": "^11.2.0",
    "@vueuse/integrations": "^11.2.0",
    "bootstrap": "^5.3.3",
    "bootstrap-vue-next": "^0.25.15",
    "js-cookie": "^3.0.5",
    "pinia": "^2.2.6",
    "vue": "^3.5.13",
    "vue-draggable-resizable": "^3.0.0",
    "vue-i18n": "^9.14.1",
    "vue-router": "^4.4.5",
    "~group-by": "workspace:*",
    "~socket.io-client-services": "workspace:*",
    "~socket.io-services": "workspace:*",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^3.9.2",
    "@eslint/eslintrc": "^3.2.0",
    "@eslint/js": "^9.15.0",
    "@intlify/eslint-plugin-vue-i18n": "^3.1.0",
    "@intlify/unplugin-vue-i18n": "^5.3.1",
    "@types/js-cookie": "^3.0.6",
    "@vitejs/plugin-vue": "^5.2.0",
    "concurrently": "^9.1.0",
    "eslint": "^9.15.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-prettier-vue": "^5.0.0",
    "eslint-plugin-vue": "^9.31.0",
    "prettier": "^3.3.3",
    "sass": "^1.81.0",
    "typescript": "^5.6.3",
    "typescript-eslint": "^8.15.0",
    "unplugin-auto-import": "^0.18.5",
    "unplugin-icons": "^0.20.1",
    "unplugin-vue-components": "^0.27.4",
    "vite": "^5.4.11",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pages": "^0.32.3",
    "vite-plugin-vue-layouts": "^0.11.0",
    "vue-tsc": "^2.1.10"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
