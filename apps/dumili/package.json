{
  "name": "~dumili",
  "version": "0.0.0",
  "engines": {
    "node": ">=18.0.0 <19.0.0"
  },
  "scripts": {
    "dumili": "concurrently -p \"[dumili-{name}]\" --kill-others-on-fail -n dm,web,web-api \"pnpm dm\" \"pnpm  -F ~dumili... -F !~prisma-clients dev\" \"pnpm -F ~dumili-api... dev\"",
    "dev": "concurrently -n vite,vue-tsc,docker-compose \"vite --port 8004\" \"vue-tsc --noEmit --watch\" \"docker compose -f docker-compose-dev.yml up\"",
    "build": "vite build",
    "preview": "vite preview",
    "typecheck": "vue-tsc --noEmit",
    "lint": "eslint --cache --fix ."
  },
  "dependencies": {
    "@cloudinary/url-gen": "^1.13.0",
    "@cloudinary/vue": "^1.11.2",
    "@vueuse/integrations": "^10.6.1",
    "async-validator": "^4.2.5",
    "axios": "^1.6.2",
    "axios-cache-interceptor": "^1.3.2",
    "bootstrap": "^5.3.2",
    "bootstrap-vue-next": "^0.15.0",
    "dayjs": "^1.11.10",
    "js-cookie": "^3.0.5",
    "page-flip": "^2.0.7",
    "pinia": "^2.1.7",
    "vue": "^3.3.8",
    "vue-i18n": "^9.7.0",
    "vue-router": "^4.2.5",
    "~api-routes": "workspace:*",
    "~axios-helper": "workspace:*",
    "~dm-types": "workspace:*",
    "~web": "workspace:*"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^1.2.1",
    "@iconify-json/bi": "^1.1.20",
    "@intlify/unplugin-vue-i18n": "^1.5.0",
    "@types/js-cookie": "^3.0.6",
    "@types/page-flip": "^2.0.3",
    "@vitejs/plugin-vue": "^4.5.0",
    "concurrently": "^8.2.2",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^8.10.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-prettier-vue": "^4.2.0",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-vue": "^9.18.1",
    "prettier": "^2.8.8",
    "sass": "^1.69.5",
    "typescript": "^5.2.2",
    "unplugin-auto-import": "^0.16.7",
    "unplugin-icons": "^0.17.4",
    "unplugin-vue-components": "^0.25.2",
    "vite": "^4.5.0",
    "vite-plugin-eslint": "^1.8.1",
    "vite-plugin-pages": "^0.31.0",
    "vue-tsc": "^1.8.22"
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}