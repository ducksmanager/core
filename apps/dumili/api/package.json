{
  "name": "~dumili-api",
  "type": "module",
  "version": "1.0.0",
  "engines": {
    "node": ">=20.0.0 <21.0.0"
  },
  "scripts": {
    "clean": "rm -rf dist",
    "build": "rm -rf dist/{api,types} && ./node_modules/.bin/tsc && tsc-alias",
    "prisma-pull-generate": "prisma db pull && prisma generate",
    "prisma-generate": "prisma generate",
    "dev": "concurrently -n typecheck,tsx \"tsc --noEmit\" \"tsx watch index.ts\""
  },
  "dependencies": {
    "@prisma/client": "^5.20.0",
    "@sentry/node": "^7.119.0",
    "axios": "^1.7.7",
    "body-parser": "^1.20.3",
    "busboy": "^1.6.0",
    "cloudinary": "^1.41.3",
    "connect-busboy": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.4.5",
    "express": "^4.21.0",
    "jsonwebtoken": "^9.0.2",
    "pdf2pic": "^3.1.3",
    "sharp": "^0.33.5",
    "socket.io": "^4.8.0",
    "~prisma-schemas": "workspace:*",
    "~socket.io-client-services": "workspace:*",
    "~socket.io-services": "workspace:*"
  },
  "devDependencies": {
    "@socket.io/admin-ui": "^0.5.1",
    "@types/body-parser": "^1.19.5",
    "@types/busboy": "^1.5.4",
    "@types/connect-busboy": "^1.0.3",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/i18n": "^0.13.12",
    "@types/jsonwebtoken": "^9.0.7",
    "@types/node": "^22.7.5",
    "concurrently": "^9.0.1",
    "prisma": "^5.20.0",
    "tsc-alias": "^1.8.10",
    "tsx": "^4.19.1",
    "typescript": "^5.6.2"
  }
}